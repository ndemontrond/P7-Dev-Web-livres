{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/extensions": "off",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto" /* this setting should be included */
            }
        ]
    }
}
